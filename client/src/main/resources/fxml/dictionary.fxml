<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.cell.*?>


<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="es.jesus.learningwords.controller.DictionaryController">
    <children>
        <TableView fx:id="wordsTable" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="-0.7864051999999901" AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn prefWidth="75.0" text="English">
                    <cellValueFactory>
                        <PropertyValueFactory property="textFrom"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="75.0" text="Spanish">
                    <cellValueFactory>
                        <PropertyValueFactory property="textTo"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="75.0" text="Learned On">
                    <cellValueFactory>
                        <PropertyValueFactory property="learnedOn"/>
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
    </children>
</AnchorPane>
